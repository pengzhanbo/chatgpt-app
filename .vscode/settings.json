    "chatgpt",
    "ChatGPTAPI",
    "hoverable",
    "Katex",
    "katexmath",
    "linkify",
    "noopener",
    "OPENAI",
    "Pinia",
    "shiki",
    "splitpanes",
    "traptitech",
